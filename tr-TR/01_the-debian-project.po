msgstr "<primary> anayasa </primary>"

msgid "<primary>Debian Project Leader</primary>"
msgstr "<primary> Debian Proje Lideri </primary>"

msgid "<primary>DPL</primary>"
msgstr "<primary> DPL </primary>"

