msgid "<emphasis>BACK TO BASICS</emphasis> Regular expression"
msgstr "<emphasis>DET GRUNNLEGGENDE</emphasis> Regulært utrykk"

msgid "<primary>regular expression</primary>"
msgstr "<primary>regulært uttrykk</primary>"

